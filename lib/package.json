{
  "name": "conekta-places-lib",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "module": "./src/index.js",
  "scripts": {
    "test": "npm run test:src && npm run test:dist",
    "test:src": "NODE_ENV=test node ./node_modules/@babel/node/bin/babel-node.js ./node_modules/.bin/mocha mocha ./src/**.test.js",
    "test:dist": "NODE_ENV=test ./node_modules/.bin/mocha mocha ./index.test.js",
    "start:dev": "NODE_ENV=dev node ./node_modules/@babel/node/bin/babel-node.js ./src/index.js",
    "build": "npm run test:src && npm run compile",
    "compile": "mkdir -p dist && rm -rf dist/*  && NODE_ENV=prod babel src --out-dir dist && npm run test:dist",
    "publish": "npm run compile"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/node": "^7.14.7",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/register": "^7.14.5",
    "assert": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^3.6.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^6.3.0",
    "eslint-scope": "^5.1.1",
    "expect": "^27.0.6",
    "mocha": "^9.0.1"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "yup": "^0.32.9"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "sourceType": "module",
      "allowImportExportEverywhere": true
    },
    "globals": {
      "__DEV__": true
    },
    "settings": {
      "lodash": {
        "version": 3
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "global-require": "off",
      "no-console": "off",
      "no-continue": "off",
      "no-underscore-dangle": "off",
      "import/no-dynamic-require": "off",
      "import/prefer-default-export": "off",
      "generator-star-spacing": "off",
      "import/no-extraneous-dependencies": "off",
      "react/no-unused-prop-types": "off",
      "max-len": "off",
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "no-throw-literal": "off",
      "no-plusplus": "off",
      "import/extensions": "off",
      "react/jsx-filename-extension": "off",
      "react/forbid-prop-types": "off",
      "react/no-danger": "off",
      "react/no-did-update-set-state": "off",
      "no-useless-escape": "off",
      "jsx-a11y/no-static-element-interactions": "warn",
      "class-methods-use-this": "off"
    }
  }
}
